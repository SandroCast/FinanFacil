-- TABLE
CREATE TABLE categorias (
    id INT AUTO_INCREMENT PRIMARY KEY,
  	titulo VARCHAR(200),
    id_usuario INT NULL,
    data_de_registro TIMESTAMP DEFAULT CURRENT_TIMESTAMP
);
CREATE TABLE lancamentos (
    id INT AUTO_INCREMENT PRIMARY KEY,
  	id_usuario INT NULL,
    descricao VARCHAR(100),
    id_categoria INT,
    valor DOUBLE,
  	fixo BOOLEAN,
  	tipo_transacao VARCHAR(20) NULL,
  	parcelas INT NULL,
  	dt_agendada DATETIME,
  	status VARCHAR(20),
    data_de_registro TIMESTAMP DEFAULT CURRENT_TIMESTAMP
);
 
